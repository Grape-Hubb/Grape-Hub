





                local keybind = "n" -- MAKE SURE IT IS LOWERCASE








                         --// DO NOT EDIT BELOW \\--




local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/nigga420-boop/lib/master/purpletheme"))()
local misc = Library:CreateTab("Misc", "Sliders, Config Speed etc.")
local univ = Library:CreateTab("Universal", "Gungame stuff")
local cred = Library:CreateTab("Credits", "wshuff, n1bu")

misc:CreateSlider("WalkSpeed", 16, 420, function(arg)
    game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = arg
end)
misc:CreateSlider("HipHeight", 0, 420, function(arg)
    game:GetService("Players").LocalPlayer.Character.Humanoid.HipHeight = arg
end)
misc:CreateSlider("JumpPower", 50, 420, function(arg)
    game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = arg
end)

misc:CreateButton("Rejoin", function()
game:GetService("TeleportService"):Teleport(game.PlaceId)
end)

univ:CreateButton("Shuff's Aimbot", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Grape-Hubb/Grape-Hub/universal-patch/esp%2C%20aimbot%2C%20chams%2C%20flight",true))()
end)

cred:CreateButton("Copy Discord Link", function()
setclipboard("https://discord.gg/p4nS6mV")
end)


local mouse = game:GetService("Players").LocalPlayer:GetMouse()
local enabled = true
mouse.KeyDown:Connect(function(key)
    if key == keybind and enabled == true then
        game:GetService("CoreGui").uiui.Enabled = false
        enabled = false
    elseif key == keybind and enabled == false then
        game:GetService("CoreGui").uiui.Enabled = true
        enabled = true
    end
end)
