sliders:CreateSlider("WalkSpeed", 16, 420, function(arg)
    game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = arg
end)
sliders:CreateSlider("HipHeight", 0, 420, function(arg)
    game:GetService("Players").LocalPlayer.Character.Humanoid.HipHeight = arg
end)
sliders:CreateSlider("JumpPower", 50, 420, function(arg)
    game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = arg
end)

sliders:CreateButton("Retarded Dex", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/IittIe/Dex/master/dex",true))()
end)

sliders:CreateButton("Rejoin", function()
game:GetService("TeleportService"):Teleport(game.PlaceId)
end)
